{
  "name": "desktop",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "main": "src/electron-starter.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-serve": "ng build --prod && electron .",
    "mac-release": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/icon.icns --prune=true --out=release-builds",
    "mac-build": "electron-installer-dmg ./release-builds/desktop-darwin-x64/desktop.app builds/desktop",
    "mac-complete-build": "electron-serve && mac-release && mac-build",
    "windows-release": "electron-packager . desktop --overwrite --platform=win64 --arch=ia64 --icon=src/assets/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Banco de Pacientes'",
    "windows-build": "node src/windows-build.js",
    "linux-release": "electron-packager . electron-tutorial-app --overwrite --platform=linux --arch=x64 --icon=src/assets/icon.png --prune=true --out=release-builds"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/fire": "^5.0.2",
    "@angular/forms": "^7.2.2",
    "@angular/http": "^7.2.2",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/router": "^7.2.2",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/email-composer": "^5.2.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^4.0.0",
    "@ionic/storage": "^2.2.0",
    "@types/jspdf": "^1.2.2",
    "cordova-plugin-email-composer": "0.9.2",
    "cordova-sqlite-storage": "3.1.0",
    "core-js": "^2.5.4",
    "electron-squirrel-startup": "^1.0.0",
    "electron-winstaller": "^3.0.4",
    "firebase": "^5.0.3",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.1.1",
    "moment": "^2.24.0",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.12.3",
    "@angular-devkit/build-angular": "~0.12.3",
    "@angular-devkit/core": "~7.2.3",
    "@angular-devkit/schematics": "~7.2.3",
    "@angular/cli": "7.2.4",
    "@angular/compiler": "~7.2.2",
    "@angular/compiler-cli": "~7.2.2",
    "@angular/language-service": "~7.2.2",
    "@ionic/angular-toolkit": "~1.4.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.0",
    "codelyzer": "~4.5.0",
    "electron": "^4.1.0",
    "electron-builder": "^20.39.0",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^13.0.1",
    "foreman": "^3.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~8.0.0",
    "tslint": "~5.12.0",
    "typescript": "~3.1.6"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-email-composer": {},
      "cordova-sqlite-storage": {}
    }
  }
}
